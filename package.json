{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@react-pdf-viewer/core": "^2.4.1",
    "@react-pdf-viewer/default-layout": "^2.6.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "aos": "^2.3.4",
    "autoprefixer": "^9.8.6",
    "axios": "^0.21.1",
    "emailjs-com": "^3.1.0",
    "gsap": "^3.7.0",
    "js-file-download": "^0.4.12",
    "npm-run-all": "^4.1.5",
    "pdfjs-dist": "^2.6.347",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-pdf": "^5.3.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "watch:sass": "sass --watch src/sass/main.scss src/css/style.css",
    "compile:sass": "sass src/sass/main.scss src/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/css/style.comp.css -o src/css/style.prefix.css",
    "compress:css": "sass --style=compressed src/css/style.prefix.css src/css/style.css",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "start": "react-scripts start",
    "build": "react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "postcss-cli": "^8.3.1"
  }
}